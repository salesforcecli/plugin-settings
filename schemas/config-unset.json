{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/ConfigResponses",
  "definitions": {
    "ConfigResponses": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Msg"
      }
    },
    "Msg": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "deprecated": {
          "type": "boolean"
        },
        "value": {
          "type": "string"
        },
        "success": {
          "type": "boolean"
        },
        "location": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "error": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "message": {
              "type": "string"
            },
            "stack": {
              "type": "string"
            }
          },
          "required": ["name", "message"],
          "additionalProperties": false
        },
        "successes": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": {}
          }
        },
        "failures": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": {}
          }
        },
        "key": {
          "type": "string"
        }
      },
      "required": ["name", "success"],
      "additionalProperties": false
    }
  }
}
